nu Align\" name=\"float\">\n            <option value=\"mx-auto\">Center</option>\n            <option value=\"floatleft\" selected>Right</option>\n            <option value=\"floatright\" right>Left</option>\n         </select>\n <input type=\"color\" title=\"Mobile Color Brand Name\" value=\"#a0e1e1\" name=\"colorbrand2\">\n    <select title=\"Brand Name on Mobile\" name=\"hideonmobile\">\n            <option value=\"d-none\">No</option>\n            <option value selected>Yes</option>\n          </select>\n           <input type=\"checkbox\" title=\"Center logo on mobile\" name=\"centeronmobile\" checked>\n \n     <input type=\"color\" title=\"BG Active & Hover\" value=\"#0c2c68\" name=\"bgactive\"> \n     <input type=\"color\" title=\"Color Active & Hover\" value=\"#ffffff\" name=\"coloractive\">      \n\n <input type=\"range\" inline title=\"Menu Padding Top & Bottom\" name=\"paddingtopbottom\" min=\"10\" max=\"30\" step=\"1\" value=\"10\">\n <input type=\"range\" inline title=\"Menu Padding Left  & Right\" name=\"paddingleftright\" min=\"10\" max=\"30\" step=\"1\" value=\"15\">\n \n    <input type=\"color\" title=\"BG Dropdown\" name=\"bgdropdown\" value=\"#ffffff\"> \n   <input type=\"color\" title=\"Color Dropdown\" name=\"colordropdown\" value=\"#000000\"> \n     <input type=\"color\" title=\"BG Menu\" name=\"menuBgColor\" value=\"#ffffff\">\n\n         <input type=\"checkbox\" title=\"Button\" name=\"showButtons\" checked>\n        <input type=\"color\" title=\"Hamburger\" name=\"hamburgerColor\" value=\"#ffffff\">\n        \n        <input type=\"range\" inline title=\"Top(Easily set menu)\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"0\">\n    <input type=\"range\" inline title=\"Bottom(Easily set menu)\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"0\">\n    </mbr-parameters>\n \n<div class=\"container-fluid nomobil bgtop displaytop\">\n        <div class=\"row gx-5 py-{{headerpadding}} align-items-center\">\n            